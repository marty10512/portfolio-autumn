@mixin desktopHd() {
  @media screen and (max-width: $bp-desktop-hd) {
    @content;
  }
}

@mixin desktop() {
  @media screen and (max-width: $bp-desktop) {
    @content;
  }
}

@mixin tablets() {
  @media screen and (max-width: $bp-tablets) {
    @content;
  }
}

@mixin phones() {
  @media screen and (max-width: $bp-phones) {
    @content;
  }
}

// colors
$white: #fff;
$orange: #dc9322;
$beige: #716d69;
$grey: #909293;
$dark-blue: #353e4e;
$link: #586ed3;
$dark-grey: #656567;